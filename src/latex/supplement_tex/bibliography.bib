@article{Pickrell2010,
abstract = {There is much interest in understanding the genetic mechanisms that underlie individual variations in gene expression. Here, RNA sequencing has been used to study gene expression in lymphoblastoid cell lines derived from Nigerian individuals for whom extensive genotype information is known. Numerous genetic determinants of variation in gene expression were identified, including variation in transcription, splicing and allele-specific expression.},
