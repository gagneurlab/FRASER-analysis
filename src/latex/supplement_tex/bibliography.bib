author = {Brechtmann, Felix and Matusevi{\v{c}}iūtė, Agnė and Mertes, Christian and Y{\'{e}}pez, Vicente A and Herzog, Maximilian and Bader, Daniel M and Prokisch, Holger and Gagneur, Julien},
doi = {10.1101/322149},
eprint = {322149},
file = {:Users/gagneurlab/Library/Application Support/Mendeley Desktop/Downloaded/Brechtmann et al. - Unknown - OUTRIDER A statistical method for detecting aberrantly expressed genes in RNA sequencing data.pdf:pdf},
