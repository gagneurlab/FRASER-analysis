pages = {291--294},
title = {{Auto-association by multilayer perceptrons and singular value decomposition}},
url = {https://link-springer-com.eaccess.ub.tum.de/content/pdf/10.1007{\%}2FBF00332918.pdf http://link.springer.com/10.1007/BF00332918},
