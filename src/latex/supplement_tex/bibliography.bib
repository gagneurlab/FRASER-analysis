pages = {4288--4297},
pmid = {22287627},
title = {{Differential expression analysis of multifactor RNA-Seq experiments with respect to biological variation}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22287627 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3378882 https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gks042},
